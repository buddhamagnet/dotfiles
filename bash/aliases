# UTILS
alias reload='. ~/.bash_profile'
alias svnstrip='find . -name .svn -print0 | xargs -0 rm -rf'
alias vim='vim -N'
# RAILS
alias r='rails'
alias routeme='rake routes | sed -e "1d" -e "s,^[^/]*,,g" | awk "{print $1}" | sort | uniq'
# RVM
alias getrvm='bash < <(curl -B http://rvm.beginrescueend.com/install/rvm)'
