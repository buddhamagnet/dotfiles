# MAKE WAY FOR OHMYZSH

alias ohmy = 'curl -L https://github.com/robbyrussell/oh-my-zsh/raw/master/tools/install.sh | sh'
alias ohmyget = 'wget --no-check-certificate https://github.com/robbyrussell/oh-my-zsh/raw/master/tools/install.sh -O - | sh'

# UTILS

alias h='history'
alias reload='. ~/.bash_profile'
alias up='cd ..'
alias f='foreman start'

# SUBVERSION SURGERY

alias svnstrip='find . -name .svn -print0 | xargs -0 rm -rf'
