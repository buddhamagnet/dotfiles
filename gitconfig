[apply]
	
	whitespace = nowarn

[alias]
	
  slog = log --pretty=oneline
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  plog = log -p
  glog = log --graph
  last = log -1 HEAD
  unstage = reset HEAD --
  last = log -1
  st = status
  co = checkout
  br = branch
  re = remote
  ls = ls-files
  mg = merge
  rb = rebase
  pu = push
  pl = pull
  ft = fetch
  cm = commit
  
[core]
  
  excludesfile = ~/.gitignore
  editor = /usr/bin/env vim

[color]
  
  ui =auto

[push]

	default = tracking
