[apply]

  whitespace = nowarn

[alias]

  br = branch
  cm = commit
  ca = commit --amend
  co = checkout
  cm = commit -v
  df = diff
  ds = diff --staged
  ft = fetch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  last = log -1 HEAD
  ls = ls-files
  logg = log --graph
  logp = log -p
  logs = log --pretty=oneline
  logr = log -g
  mg = merge
  pl = pull
  pu = push
  re = remote
  rea = remote add
  rem = remote rm
  ren = remote renames
  res = remote show
  rev = remote -v
  rb = rebase
  rh = reset HEAD
  rmc = rm --cached
  sh = show
  st = status
  t = tag
  ta = tag -a
  tl = tag -l
  ts = tag -s
  tv = tag -v
  unstage = reset HEAD --
  premerge = reset --hard ORIG_HEAD

[core]

  excludesfile = ~/.gitignore
  editor = /usr/bin/env vim
  autocrlf = input

[commit]

  template = ~/.gitcommit.txt

[color]

  ui = true

[push]

  default = tracking

[mergetool]

  keepBackup = true

[help]

  autocorrect = 1
