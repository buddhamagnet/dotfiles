[apply]

  whitespace = nowarn

[alias]
  
  br = branch
  cm = commit
  co = checkout
  df = diff
  ft = fetch
  glog = log --graph
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  last = log -1 HEAD
  ls = ls-files
  mg = merge
  pl = pull
  plog = log -p
  pu = push
  re = remote
  rb = rebase
  slog = log --pretty=oneline
  st = status
  unstage = reset HEAD --
  
[core]
  
  excludesfile = ~/.gitignore
  editor = /usr/bin/env vim

[color]
  
  ui =auto

[push]

  default = tracking
