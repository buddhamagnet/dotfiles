[apply]
	
	whitespace = nowarn

[alias]
	
  slog = log --pretty=oneline
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  plog = log -p
  glog = log --graph
  last = log -1 HEAD
  unstage = reset HEAD --
    
[core]
  
  excludesfile = ~/.gitignore
  editor = /usr/bin/env vim
  autocrlf = input
  safecrlf = true

[color]
  
  ui =auto

[push]

	default = tracking
